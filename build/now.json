{
  "version": 2,
  "name": "SpanishMedTerms",
  "alias": "SpanishMedTerms",
  "build": {
    "env": {
      "REACT_APP_API_BASE": "https://learn-spanish-med-terms-api.herokuapp.com"
    }
  },
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/site.webmanifest.json$",
      "dest": "/site.webmanifest.json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
 }